# Bundler config files
# https://github.com/bundler/bundler/issues/4523#issuecomment-216694527
/server/.bundle

# Rails log files
/server/log/*

# Rails temporary files
/server/tmp/*

# Rails test coverage generated files
/server/coverage/*

# built client files after copying them to the server
/server/public/asset-manifest.json
/server/public/favicon.ico
/server/public/index.html
/server/public/manifest.json
/server/public/service-worker.js
/server/public/static/css/main.*.chunk.css
/server/public/static/css/vendors.*.chunk.css
/server/public/static/js/main.*.chunk.js
/server/public/static/js/main.*.chunk.js.map
/server/public/static/js/runtime~main.*.js
/server/public/static/js/runtime~main.*.js.map
/server/public/static/js/vendors.*.chunk.js
/server/public/static/js/vendors.*.chunk.js.map
/server/public/static/media

# byebug history file
/server/.byebug_history

# Rails production and staging credentials.yml.enc file key, used to decrypt the file
/server/config/credentials/staging.key
/server/config/credentials/production.key

# empty files to keep the directories inside git even when empty
!/server/log/.keep
!/server/tmp/.keep
/.vscode/

# node_modules for the client
/client/node_modules

# coverage files generated by jest
/client/coverage

# build client files
/client/build

# generated by Finder on macOS
.DS_Store

# env files and variations
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# npm and yarn log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# SQL dump files (they can have any extension, but .dump may be used
